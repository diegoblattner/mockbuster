{
  "name": "webapp",
  "version": "0.0.0",
  "private": true,
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "license": "ISC",
  "author": "diblattner@gmail.com",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:client": "tsc -b && vite build --emptyOutDir --outDir ../server/dist/client",
    "build:client:hydrate": "tsc -b && VITE_HYDRATE=TRUE vite build --emptyOutDir --outDir ../server/dist/client",
    "build:client:hydrate:ssr": "tsc -b && VITE_HYDRATE=TRUE vite build --ssr --emptyOutDir --outDir ../server/dist/ssr",
    "build:for-server:dev": "npm run build:client:hydrate",
    "build:for-server:build": "npm run build:client:hydrate && npm run build:client:hydrate:ssr",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
